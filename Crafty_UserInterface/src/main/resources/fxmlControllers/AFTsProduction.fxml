<VBox xmlns="http://javafx.com/javafx/21"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.cesr.crafty.controller.fxml.AFTsProductionController">
	<children>
		<TitledPane animated="true"
			text="Optimal Production Levels and Behevoir Parametrs">
			<content>
				<VBox spacing="10.0">
					<children>
						<BarChart fx:id="histogramePlevel">
							<xAxis>
								<CategoryAxis side="BOTTOM" />
							</xAxis>
							<yAxis>
								<NumberAxis side="LEFT" />
							</yAxis>
						</BarChart>
					</children>
				</VBox>
			</content>
		</TitledPane>
		<TitledPane animated="true"
			text="Productivity frequency for a sample of random capital values (1,000 points).">
			<content>
				<VBox fx:id="boxCharts"  />
			</content>
		</TitledPane>

	</children>
</VBox>
